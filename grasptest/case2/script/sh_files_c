0
y
1s(2,i)2p(1,i)

2s,2p
1,3
0
n
EOF

cp rcsf.out DF.c

#######################################

rcsfgenerate << EOF
*
0
1s(2,*)2s(1,*)

5s,5p,5d,5f,5g
1,1
3
n
EOF

cp rcsf.out even.c

rcsfsplit << EOF
even
3
3s,3p,3d
3
4s,4p,4d,4f
4
5s,5p,5d,5f,5g
5
EOF

################################

rcsfgenerate << EOF
*
0
1s(2,*)2p(1,*)

5s,5p,5d,5f,5g
1,3
3
n
EOF

cp rcsf.out odd.c

rcsfsplit << EOF
odd
3
3s,3p,3d
3
4s,4p,4d,4f
4
5s,5p,5d,5f,5g
5
EOF
